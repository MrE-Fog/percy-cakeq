{
  "name": "percy-cake-hydration-tools",
  "version": "1.0.10",
  "description": "Command line hydration utility scripts for files dehydrated for `Percy - Configuration As Kode Editor`",
  "repository": "https://github.com/tmobile/percy-cake",
  "main": "dist/index.js",
  "types": "dist/types/index.d.js",
  "bin": {
    "hydrate": "dist/hydrate.js",
    "compare-json": "dist/compare-json.js"
  },
  "scripts": {
    "docs": "typedoc --out docs/ --excludePrivate --exclude '**/index.ts' src/lib",
    "tsc": "tsc",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "jest",
    "compare-json": "ts-node src/compare-json.ts",
    "hydrate": "ts-node src/hydrate.ts",
    "compare-json.js": "node dist/compare-json.js",
    "hydrate.js": "node dist/hydrate.js"
  },
  "dependencies": {
    "minimist": "1.2.5",
    "config": "^3.0.1",
    "fs-extra": "^9.1.0",
    "js-yaml": "^4.1.0",
    "jsondiffpatch": "^0.4.1",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "rimraf": "^3.0.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/config": "0.0.38",
    "@types/jest": "^26.0.23",
    "@types/minimist": "^1.2.0",
    "@types/rimraf": "^3.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
